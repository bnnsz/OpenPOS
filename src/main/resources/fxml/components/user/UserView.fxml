<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root id="AnchorPane" alignment="TOP_CENTER" prefWidth="475.0" styleClass="contentPanel" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox maxHeight="1.7976931348623157E308" prefWidth="100.0" styleClass="body">
         <children>
            <TabPane maxHeight="1.7976931348623157E308" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Account ">
                  <content>
                    <AnchorPane maxHeight="1.7976931348623157E308" minHeight="0.0" minWidth="0.0">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
                                 <children>
                                    <TextField fx:id="username" promptText="Username" styleClass="input-field" />
                                    <TextField fx:id="firstName" promptText="First Name" styleClass="input-field" />
                                    <TextField fx:id="lastName" promptText="Last Name" styleClass="input-field" />
                                    <TextField fx:id="otherNames" promptText="Other names" styleClass="input-field" />
                                    <TextField fx:id="email" promptText="Email" styleClass="input-field" />
                                    <TextField fx:id="phone" promptText="Phone Number" styleClass="input-field" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-round" />
                        <String fx:value="btn-flat" />
                        <String fx:value="btn-default" />
                     </styleClass>
                </Tab>
                <Tab text="Roles">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 <children>
                                    <VBox fx:id="rolesVBox" alignment="CENTER" minHeight="0.0" prefWidth="100.0">
                                       <VBox.margin>
                                          <Insets bottom="20.0" top="20.0" />
                                       </VBox.margin>
                                    </VBox>
                                    <HBox maxHeight="35.0" minHeight="30.0" spacing="10.0">
                                       <children>
                                          <ComboBox fx:id="roleComboBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" HBox.hgrow="ALWAYS" />
                                          <Button fx:id="addRoleButton" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" maxHeight="1.7976931348623157E308" mnemonicParsing="false" prefWidth="35.0">
                                             <styleClass>
                                                <String fx:value="btn" />
                                                <String fx:value="btn-default" />
                                                <String fx:value="btn-flat" />
                                                <String fx:value="btn-round" />
                                             </styleClass>
                                             <graphic>
                                                <ImageView fx:id="addRoleButtonIcon" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
                                             </graphic>
                                          </Button>
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
                     <styleClass>
                        <String fx:value="btn-round" />
                        <String fx:value="btn-flat" />
                        <String fx:value="btn" />
                        <String fx:value="btn-default" />
                     </styleClass>
                </Tab>
              </tabs>
            </TabPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER" prefWidth="200.0" spacing="10.0" styleClass="footer">
         <children>
            <Button fx:id="cancelButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="CANCEL" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-danger" />
                  <String fx:value="btn-flat" />
                  <String fx:value="btn-round" />
               </styleClass>
            </Button>
            <Button fx:id="saveButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="35.0" text="SAVE" HBox.hgrow="ALWAYS">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-default" />
                  <String fx:value="btn-flat" />
                  <String fx:value="btn-round" />
               </styleClass>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
</fx:root>
