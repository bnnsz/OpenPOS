<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root id="AnchorPane" alignment="CENTER" maxWidth="1.7976931348623157E308" prefWidth="600.0" style="-fx-border-width: 0 0 1 0; -fx-border-color: -theme-primary-background-dark;" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="nameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="35.0" text="Name" HBox.hgrow="ALWAYS" />
      <Button fx:id="removeButton" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" maxHeight="32.0" mnemonicParsing="false" text="Button">
         <graphic>
            <ImageView fx:id="removeButtonIcon" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
         </graphic>
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-danger" />
            <String fx:value="btn-round" />
            <String fx:value="btn-outline" />
         </styleClass>
      </Button>
   </children>
</fx:root>
